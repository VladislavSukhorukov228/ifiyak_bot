import os
import telebot
from telebot.types import InlineKeyboardMarkup, InlineKeyboardButton, ReplyKeyboardMarkup, KeyboardButton

# –¢–û–ö–ï–ù –î–õ–Ø –ë–û–¢–ê
TOKEN = os.getenv('BOT_TOKEN')
bot = telebot.TeleBot(TOKEN)

# Placeholder for unanswered questions
PLACEHOLDER = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –ø–æ —ç—Ç–æ–º—É –≤–æ–ø—Ä–æ—Å—É –æ–±–Ω–æ–≤–ª—è–µ—Ç—Å—è. –û–Ω–∞ –±—É–¥–µ—Ç –æ–ø—É–±–ª–∏–∫–æ–≤–∞–Ω–∞ –±–ª–∏–∂–µ –∫ –Ω–∞—á–∞–ª—É –ø—Ä–∏–µ–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏. –†–µ–∫–æ–º–µ–Ω–¥—É–µ–º —Å–ª–µ–¥–∏—Ç—å –∑–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è–º–∏ –Ω–∞ —Å–∞–π—Ç–µ –°–§–£."

# Links to PDFs (updated with new URL)
POLOZHENIE_PDF = "https://ifiyak.sfu-kras.ru/sites/default/files/content/2025/o%20raspredelenii%20po%20azykovym%20gruppam.pdf"

# Helper function to create main menu keyboard
def main_menu_keyboard():
    markup = ReplyKeyboardMarkup(resize_keyboard=True, row_width=2)
    buttons = [
        KeyboardButton("üìù –ü–æ–¥–∞—á–∞ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤"),
        KeyboardButton("üí∞ –ü–ª–∞—Ç–Ω–æ–µ –æ–±—É—á–µ–Ω–∏–µ"),
        KeyboardButton("üè† –û–±—â–µ–∂–∏—Ç–∏–µ"),
        KeyboardButton("üéì –£—á–µ–±–Ω—ã–π –ø—Ä–æ—Ü–µ—Å—Å"),
        KeyboardButton("üåç –í—ã–±–æ—Ä —è–∑—ã–∫–æ–≤—ã—Ö –≥—Ä—É–ø–ø"),
        KeyboardButton("üìû –ö–æ–Ω—Ç–∞–∫—Ç—ã –ø—Ä–∏–µ–º–Ω–æ–π –∫–æ–º–∏—Å—Å–∏–∏")
    ]
    markup.add(*buttons)
    return markup

# Helper function for back and home buttons (inline)
def back_home_buttons(back_data='main', home_data='main'):
    markup = InlineKeyboardMarkup(row_width=2)
    markup.add(
        InlineKeyboardButton("üîô –ù–∞–∑–∞–¥", callback_data=back_data),
        InlineKeyboardButton("üè† –í –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data=home_data)
    )
    markup.add(
        InlineKeyboardButton("üìû –°–≤—è–∑–∞—Ç—å—Å—è —Å –∫–æ–º–∏—Å—Å–∏–µ–π", callback_data='contacts')
    )
    return markup

# Start command handler
@bot.message_handler(commands=['start'])
def send_welcome(message):
    text = """–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ! –ú—ã —Ä–∞–¥—ã, —á—Ç–æ –≤—ã –≤—ã–±—Ä–∞–ª–∏ –ò–§–∏–Ø–ö –°–§–£ –¥–ª—è –ø–æ—Å—Ç—É–ø–ª–µ–Ω–∏—è. –Ø ‚Äî —Ü–∏—Ñ—Ä–æ–≤–æ–π –ø–æ–º–æ—â–Ω–∏–∫, –∫–æ—Ç–æ—Ä—ã–π –ø–æ–º–æ–∂–µ—Ç –í–∞–º —Ä–∞–∑–æ–±—Ä–∞—Ç—å—Å—è –≤ –ø—Ä–∞–≤–∏–ª–∞—Ö –ø—Ä–∏–µ–º–∞ –∏ –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç—è—Ö —É—á–µ–±—ã –≤ –Ω–∞—à–µ–º –ò–Ω—Å—Ç–∏—Ç—É—Ç–µ.
–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤—ã–±–µ—Ä–∏—Ç–µ —Ç–µ–º—É, –∫–æ—Ç–æ—Ä–∞—è –í–∞—Å –∏–Ω—Ç–µ—Ä–µ—Å—É–µ—Ç:"""
    bot.send_message(message.chat.id, text, reply_markup=main_menu_keyboard())

# Handle text messages for main menu selections
@bot.message_handler(func=lambda message: True)
def handle_text(message):
    text = message.text.strip()
    if text == "üìù –ü–æ–¥–∞—á–∞ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤":
        handle_documents(message.chat.id)
    elif text == "üí∞ –ü–ª–∞—Ç–Ω–æ–µ –æ–±—É—á–µ–Ω–∏–µ":
        handle_paid_education(message.chat.id)
    elif text == "üè† –û–±—â–µ–∂–∏—Ç–∏–µ":
        handle_dormitory(message.chat.id)
    elif text == "üéì –£—á–µ–±–Ω—ã–π –ø—Ä–æ—Ü–µ—Å—Å":
        handle_educational_process(message.chat.id)
    elif text == "üåç –í—ã–±–æ—Ä —è–∑—ã–∫–æ–≤—ã—Ö –≥—Ä—É–ø–ø":
        handle_language_groups(message.chat.id)
    elif text == "üìû –ö–æ–Ω—Ç–∞–∫—Ç—ã –ø—Ä–∏–µ–º–Ω–æ–π –∫–æ–º–∏—Å—Å–∏–∏":
        handle_contacts(message.chat.id)
    else:
        unknown_query(message.chat.id)

# Unknown query response
def unknown_query(chat_id):
    text = "–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —è –ø–æ–∫–∞ –Ω–µ –∑–Ω–∞—é –æ—Ç–≤–µ—Ç–∞ –Ω–∞ —ç—Ç–æ—Ç –≤–æ–ø—Ä–æ—Å. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –≤—ã–±—Ä–∞—Ç—å –æ–¥–Ω—É –∏–∑ —Ç–µ–º –≤ –º–µ–Ω—é –Ω–∏–∂–µ –∏–ª–∏ –ø–æ–∑–≤–æ–Ω–∏—Ç–µ –≤ –ø—Ä–∏–µ–º–Ω—É—é –∫–æ–º–∏—Å—Å–∏—é –ø–æ —Ç–µ–ª–µ—Ñ–æ–Ω—É +79230153878."
    bot.send_message(chat_id, text, reply_markup=main_menu_keyboard())

# Callback query handler for inline buttons
@bot.callback_query_handler(func=lambda call: True)
def callback_query(call):
    data = call.data
    if data == 'main':
        send_welcome(call.message)
    elif data == 'contacts':
        handle_contacts(call.message.chat.id)
    # Documents section
    elif data.startswith('docs_'):
        handle_documents_sub(call.message.chat.id, data)
    # Paid education
    elif data.startswith('paid_'):
        handle_paid_sub(call.message.chat.id, data)
    # Dormitory
    elif data.startswith('dorm_'):
        handle_dorm_sub(call.message.chat.id, data)
    # Educational process
    elif data.startswith('edu_'):
        handle_edu_sub(call.message.chat.id, data)
    # Language groups
    elif data.startswith('lang_'):
        handle_lang_sub(call.message.chat.id, data)
    bot.answer_callback_query(call.id)

# Section: –ü–æ–¥–∞—á–∞ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤
def handle_documents(chat_id):
    text = "–ó–¥–µ—Å—å —Å–æ–±—Ä–∞–Ω—ã –æ—Ç–≤–µ—Ç—ã –Ω–∞ –≤–æ–ø—Ä–æ—Å—ã –æ —Ç–æ–º, –∫–∞–∫ –∏ –∫–æ–≥–¥–∞ –ø–æ–¥–∞–≤–∞—Ç—å –¥–æ–∫—É–º–µ–Ω—Ç—ã. –ß—Ç–æ –∏–º–µ–Ω–Ω–æ –í—ã —Ö–æ—Ç–∏—Ç–µ —É–∑–Ω–∞—Ç—å?"
    markup = InlineKeyboardMarkup(row_width=1)
    buttons = [
        InlineKeyboardButton("–°—Ä–æ–∫–∏ –∏ —Å–ø–æ—Å–æ–±—ã –ø–æ–¥–∞—á–∏", callback_data='docs_terms'),
        InlineKeyboardButton("–î–æ–∫—É–º–µ–Ω—Ç—ã", callback_data='docs_docs'),
        InlineKeyboardButton("–ï–ì–≠, –º–∏–Ω–∏–º–∞–ª—å–Ω—ã–µ –±–∞–ª–ª—ã, –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –º–µ—Å—Ç", callback_data='docs_ege'),
        InlineKeyboardButton("–ò–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω—ã–µ –¥–æ—Å—Ç–∏–∂–µ–Ω–∏—è", callback_data='docs_achievements'),
        InlineKeyboardButton("–õ—å–≥–æ—Ç—ã –∏ –∫–≤–æ—Ç—ã", callback_data='docs_benefits'),
        InlineKeyboardButton("–ü–æ—Å—Ç—É–ø–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ –°–ü–û", callback_data='docs_after_spo'),
        InlineKeyboardButton("–ü—Ä–∞–≤–∏–ª–∞ –∑–∞—á–∏—Å–ª–µ–Ω–∏—è –∏ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—ã", callback_data='docs_rules')
    ]
    markup.add(*buttons)
    markup.add(InlineKeyboardButton("üîô –ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data='main'))
    bot.send_message(chat_id, text, reply_markup=markup)

def handle_documents_sub(chat_id, data):
    if data == 'docs_terms':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –≤–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ö–æ–≥–¥–∞ –Ω–∞—á–∏–Ω–∞–µ—Ç—Å—è –ø–æ–¥–∞—á–∞ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤?", callback_data='docs_terms_start'),
            InlineKeyboardButton("–ö–æ–≥–¥–∞ –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å—Ä–æ–∫ –ø–æ–¥–∞—á–∏ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤?", callback_data='docs_terms_end'),
            InlineKeyboardButton("–ù—É–∂–Ω–æ –ª–∏ –ø—Ä–∏–µ–∑–∂–∞—Ç—å –≤ —É–Ω–∏–≤–µ—Ä—Å–∏—Ç–µ—Ç –¥–ª—è –ø–æ–¥–∞—á–∏ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤?", callback_data='docs_terms_visit'),
            InlineKeyboardButton("–ú–æ–∂–Ω–æ –ª–∏ —É—á–∞—Å—Ç–≤–æ–≤–∞—Ç—å –≤ –∫–æ–Ω–∫—É—Ä—Å–µ –Ω–∞ –ø–ª–∞—Ç–Ω–æ–π –æ—Å–Ω–æ–≤–µ –∏ –Ω–∞ –±—é–¥–∂–µ—Ç –æ–¥–Ω–æ–≤—Ä–µ–º–µ–Ω–Ω–æ?", callback_data='docs_terms_both')
        )
        markup.add(*back_home_buttons('docs_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'docs_terms_start':
        text = "–ù–∞—á–∞–ª–æ –ø—Ä–∏—ë–º–∞ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤, –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –¥–ª—è –ø–æ—Å—Ç—É–ø–ª–µ–Ω–∏—è –Ω–∞ –≤—Å–µ —Ñ–æ—Ä–º—ã –æ–±—É—á–µ–Ω–∏—è: 20 –∏—é–Ω—è 2026 –≥–æ–¥–∞ –≤ 10:00 –ø–æ –º–µ—Å—Ç–Ω–æ–º—É –≤—Ä–µ–º–µ–Ω–∏ (–≤ 06:00 –ø–æ –º–æ—Å–∫–æ–≤—Å–∫–æ–º—É –≤—Ä–µ–º–µ–Ω–∏). –†–µ–∫–æ–º–µ–Ω–¥—É–µ–º –∏–∑—É—á–∏—Ç—å –∫–∞–ª–µ–Ω–¥–∞—Ä—å –ø—Ä–∏–µ–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏ –Ω–∞ —Å–∞–π—Ç–µ —É–Ω–∏–≤–µ—Ä—Å–∏—Ç–µ—Ç–∞."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–ö–∞–ª–µ–Ω–¥–∞—Ä—å –ø—Ä–∏–µ–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏", url="https://sfu.ru/ru/admission"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('docs_terms', 'main'))
    elif data == 'docs_terms_end':
        text = "25 –∏—é–ª—è –≤ 17:00 –ø–æ –º–µ—Å—Ç–Ω–æ–º—É –≤—Ä–µ–º–µ–Ω–∏ (–≤ 13:00 –ø–æ –º–æ—Å–∫–æ–≤—Å–∫–æ–º—É –≤—Ä–µ–º–µ–Ω–∏). –†–µ–∫–æ–º–µ–Ω–¥—É–µ–º –∏–∑—É—á–∏—Ç—å –∫–∞–ª–µ–Ω–¥–∞—Ä—å –ø—Ä–∏–µ–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏ –Ω–∞ —Å–∞–π—Ç–µ —É–Ω–∏–≤–µ—Ä—Å–∏—Ç–µ—Ç–∞."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–ö–∞–ª–µ–Ω–¥–∞—Ä—å –ø—Ä–∏–µ–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏", url="https://sfu.ru/ru/admission"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('docs_terms', 'main'))
    elif data == 'docs_terms_visit':
        text = "–≠—Ç–æ –Ω–µ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ. –ß–µ—Ä–µ–∑ –ø–æ—Ä—Ç–∞–ª ¬´–ì–æ—Å—É—Å–ª—É–≥–∏¬ª –º–æ–∂–Ω–æ –ø–æ–¥–∞—Ç—å –∑–∞—è–≤–ª–µ–Ω–∏–µ –æ –ø—Ä–∏—ë–º–µ –Ω–∞ –±—é–¥–∂–µ—Ç –∏ –ø–ª–∞—Ç–Ω–æ–µ, —Å–æ–≥–ª–∞—Å–∏–µ –Ω–∞ –∑–∞—á–∏—Å–ª–µ–Ω–∏–µ –Ω–∞ –±—é–¥–∂–µ—Ç, –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ –≤—Å—Ç—É–ø–∏—Ç–µ–ª—å–Ω—ã–µ –∏—Å–ø—ã—Ç–∞–Ω–∏—è. –ó–∞—è–≤–ª–µ–Ω–∏–µ –ø–æ–¥–∞—ë—Ç—Å—è –∞–±–∏—Ç—É—Ä–∏–µ–Ω—Ç–æ–º —á–µ—Ä–µ–∑ –ª–∏—á–Ω—É—é —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å. –í —Å–ª—É—á–∞–µ –ø–æ–¥–∞—á–∏ –∑–∞—è–≤–ª–µ–Ω–∏—è —á–µ—Ä–µ–∑ —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å —Ä–æ–¥–∏—Ç–µ–ª—è –∏–ª–∏ –∏–Ω–æ–≥–æ –ª–∏—Ü–∞ –∑–∞—è–≤–ª–µ–Ω–∏–µ –±—É–¥–µ—Ç –æ—Ç–∫–ª–æ–Ω–µ–Ω–æ. –ü–æ–¥—Ä–æ–±–Ω–µ–µ –Ω–∞ —Å–∞–π—Ç–µ. –î–æ–∫—É–º–µ–Ω—Ç—ã —Ç–∞–∫–∂–µ –º–æ–∂–Ω–æ –æ—Ç–ø—Ä–∞–≤–∏—Ç—å –ø–æ—á—Ç–æ–π –Ω–∞ –∞–¥—Ä–µ—Å: 660041, –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–∏–π –∫—Ä–∞–π, –≥.–æ. –≥–æ—Ä–æ–¥ –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫, –≥ –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫, –ø—Ä-–∫—Ç –°–≤–æ–±–æ–¥–Ω—ã–π, –¥. 79,  –û—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–º—É —Å–µ–∫—Ä–µ—Ç–∞—Ä—é –ü—Ä–∏—ë–º–Ω–æ–π –∫–æ–º–∏—Å—Å–∏–∏. –î–æ–∫—É–º–µ–Ω—Ç—ã –Ω–∞–ø—Ä–∞–≤–ª—è—é—Ç—Å—è —Å —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ–º –æ –≤—Ä—É—á–µ–Ω–∏–∏ –∏ –æ–ø–∏—Å—å—é –≤–ª–æ–∂–µ–Ω–∏—è."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–ü–æ–¥—Ä–æ–±–Ω–µ–µ –æ –ø–æ–¥–∞—á–µ —á–µ—Ä–µ–∑ –ì–æ—Å—É—Å–ª—É–≥–∏", url="https://sfu.ru/ru/admission/bachelor/how-to-apply"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('docs_terms', 'main'))
    elif data == 'docs_terms_both':
        text = "–î–∞, –º–æ–∂–Ω–æ."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('docs_terms', 'main'))
    elif data == 'docs_docs':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ù—É–∂–µ–Ω –ª–∏ –æ—Ä–∏–≥–∏–Ω–∞–ª –∞—Ç—Ç–µ—Å—Ç–∞—Ç–∞ –¥–ª—è –ø–æ–¥–∞—á–∏ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤?", callback_data='docs_docs_original'),
            InlineKeyboardButton("–ú–Ω–µ –µ—â–µ –Ω–µ –≤—ã–¥–∞–ª–∏ –∞—Ç—Ç–µ—Å—Ç–∞—Ç, –º–æ–∂–Ω–æ –ª–∏ –ø–æ–¥–∞—Ç—å –¥–æ–∫—É–º–µ–Ω—Ç—ã –±–µ–∑ –Ω–µ–≥–æ?", callback_data='docs_docs_no_attestat')
        )
        markup.add(*back_home_buttons('docs_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'docs_docs_original':
        text = "–ù–∞ –º–æ–º–µ–Ω—Ç –ø–æ–¥–∞—á–∏ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤ –æ—Ä–∏–≥–∏–Ω–∞–ª –Ω–µ —Ç—Ä–µ–±—É–µ—Ç—Å—è. –û–¥–Ω–∞–∫–æ –æ–Ω –ø–æ–Ω–∞–¥–æ–±–∏—Ç—Å—è –ø–æ–∑–∂–µ –¥–ª—è –∑–∞—á–∏—Å–ª–µ–Ω–∏—è."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('docs_docs', 'main'))
    elif data == 'docs_docs_no_attestat':
        text = "–î–∞, –Ω–æ —Ç–æ–ª—å–∫–æ —á–µ—Ä–µ–∑ ¬´–ì–æ—Å—É—Å–ª—É–≥–∏¬ª. –ü–æ—Å–ª–µ –ø–æ–ª—É—á–µ–Ω–∏—è –∞—Ç—Ç–µ—Å—Ç–∞—Ç–∞ –¥–∞–Ω–Ω—ã–µ –æ –Ω–µ–º –±—É–¥—É—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –∑–∞–≥—Ä—É–∂–µ–Ω—ã."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('docs_docs', 'main'))
    elif data == 'docs_ege':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–° –∫–∞–∫–∏–º–∏ –ï–ì–≠ —è –º–æ–≥—É –ø–æ—Å—Ç—É–ø–∏—Ç—å –≤ –ò–§–∏–Ø–ö?", callback_data='docs_ege_subjects'),
