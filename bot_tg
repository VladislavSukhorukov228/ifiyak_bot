            InlineKeyboardButton("–ö–∞–∫ —É–∑–Ω–∞—Ç—å, –≤ –∫–∞–∫–æ–µ –∏–º–µ–Ω–Ω–æ –æ–±—â–µ–∂–∏—Ç–∏–µ –º–µ–Ω—è –∑–∞—Å–µ–ª—è—Ç?", callback_data='dorm_docs_which'),
            InlineKeyboardButton("–ö–∞–∫–∏–µ –¥–æ–∫—É–º–µ–Ω—Ç—ã –Ω—É–∂–Ω—ã –¥–ª—è –∑–∞—Å–µ–ª–µ–Ω–∏—è?", callback_data='dorm_docs_needed'),
            InlineKeyboardButton("–í –∫–∞–∫–∏–µ –¥–∞—Ç—ã –∏ –≤ –∫–∞–∫–æ–µ –≤—Ä–µ–º—è –º–æ–∂–Ω–æ –±—É–¥–µ—Ç –∑–∞—Å–µ–ª–∏—Ç—å—Å—è?", callback_data='dorm_docs_dates'),
            InlineKeyboardButton("–ú–æ–∂–Ω–æ –ª–∏ —Ä–æ–¥–∏—Ç–µ–ª—è–º –∑–∞–π—Ç–∏ –≤ –∫–æ–º–Ω–∞—Ç—É –∏ –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å —É—Å–ª–æ–≤–∏—è –ø—Ä–∏ –∑–∞—Å–µ–ª–µ–Ω–∏–∏?", callback_data='dorm_docs_parents')
        )
        markup.add(*back_home_buttons('dorm_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'dorm_docs_which':
        text = "–ü–æ—Å–ª–µ –≤—ã—Ö–æ–¥–∞ –ø—Ä–∏–∫–∞–∑–æ–≤ –æ –∑–∞—á–∏—Å–ª–µ–Ω–∏–∏ –í–∞–º –Ω–∞ –ø–æ—á—Ç—É (–∫–æ—Ç–æ—Ä—É—é –í—ã —É–∫–∞–∑–∞–ª–∏ –ø—Ä–∏ –ø–æ–¥–∞—á–µ –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤) –≤ —Ç–µ—á–µ–Ω–∏–µ 2 —Ä–∞–±–æ—á–∏—Ö –¥–Ω–µ–π –ø—Ä–∏–¥—ë—Ç –ø–∏—Å—å–º–æ, –≥–¥–µ –ø–æ–¥—Ä–æ–±–Ω–æ –±—É–¥–µ—Ç —É–∫–∞–∑–∞–Ω–∞ –≤—Å—è –≤–∞–∂–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –ø–æ –∑–∞—Å–µ–ª–µ–Ω–∏—é ‚Äì –Ω–æ–º–µ—Ä –æ–±—â–µ–∂–∏—Ç–∏—è –∏ –∫–æ–º–Ω–∞—Ç—ã, –∞ —Ç–∞–∫–∂–µ –ø–µ—Ä–µ—á–µ–Ω—å –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –¥–ª—è –∑–∞—Å–µ–ª–µ–Ω–∏—è –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_docs', 'main'))
    elif data == 'dorm_docs_needed':
        text1 = "–í –¥–µ–Ω—å –∑–∞—Å–µ–ª–µ–Ω–∏—è –≤ –æ–±—â–µ–∂–∏—Ç–∏–µ —Å —Å–æ–±–æ–π –Ω—É–∂–Ω–æ –∏–º–µ—Ç—å —Å–ª–µ–¥—É—é—â–∏–π –ø–∞–∫–µ—Ç –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤: 1. –û—Ä–∏–≥–∏–Ω–∞–ª –ø–∞—Å–ø–æ—Ä—Ç–∞; 2. –ö–æ–ø–∏—é –ø–∞—Å–ø–æ—Ä—Ç–∞ (–±–µ–∑ –ø—Ä–æ–ø–∏—Å–∫–∏ –≤ –≥. –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–µ); 3. 6 —Ñ–æ—Ç–æ–≥—Ä–∞—Ñ–∏–π (3—Ö4);"
        text2 = "4. –°–ø—Ä–∞–≤–∫—É –æ –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏–∏ —Ñ–ª—é–æ—Ä–æ–≥—Ä–∞—Ñ–∏–∏ (—Å—Ä–æ–∫ –¥–µ–π—Å—Ç–≤–∏—è —Å–ø—Ä–∞–≤–∫–∏ ‚Äì –æ–¥–∏–Ω –≥–æ–¥); 5. –ö–æ–ø–∏—é –°–ù–ò–õ–°; 6. –ö–æ–ø–∏—é –ò–ù–ù (–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –∑–Ω–∞—Ç—å –Ω–æ–º–µ—Ä)."
        bot.send_message(chat_id, text1)
        bot.send_message(chat_id, text2, reply_markup=back_home_buttons('dorm_docs', 'main'))
    elif data == 'dorm_docs_dates':
        text = "–ó–∞—Å–µ–ª–µ–Ω–∏–µ –∏–Ω–æ–≥–æ—Ä–æ–¥–Ω–∏—Ö —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ 1 –∫—É—Ä—Å–∞ –ø—Ä–æ–∏—Å—Ö–æ–¥–∏—Ç –¥–æ –Ω–∞—á–∞–ª–∞ —É—á–µ–±–Ω–æ–≥–æ –ø—Ä–æ—Ü–µ—Å—Å–∞ –≤ –ø–æ—Å–ª–µ–¥–Ω–∏–µ –¥–Ω–∏ –∞–≤–≥—É—Å—Ç–∞. –°–ª–µ–¥–∏—Ç–µ –∑–∞ –Ω–æ–≤–æ—Å—Ç—è–º–∏ –Ω–∞ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω–æ–º —Å–∞–π—Ç–µ –ò–§–∏–Ø–ö –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –±–æ–ª–µ–µ —Ç–æ—á–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("O—Ñ–∏—Ü–∏–∞–ª—å–Ω—ã–π —Å–∞–π—Ç –ò–§–∏–Ø–ö", url="https://ifiyak.sfu-kras.ru/"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('dorm_docs', 'main'))
    elif data == 'dorm_docs_parents':
        text = "–í –¥–µ–Ω—å –∑–∞—Å–µ–ª–µ–Ω–∏—è –¥–æ—Å—Ç—É–ø –≤ –æ–±—â–µ–∂–∏—Ç–∏—è –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è —Ç–æ–ª—å–∫–æ —Å—Ç—É–¥–µ–Ω—Ç–∞–º –¥–ª—è –æ—Ñ–æ—Ä–º–ª–µ–Ω–∏—è –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤. –†–æ–¥–∏—Ç–µ–ª—è–º –¥–æ—Å—Ç—É–ø –±—É–¥–µ—Ç –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω –ø–æ—Å–ª–µ —Ç–æ–≥–æ, –∫–∞–∫ –æ—Å–Ω–æ–≤–Ω–æ–π –ø–æ—Ç–æ–∫ —Å—Ç—É–¥–µ–Ω—Ç–æ–≤ –±—É–¥–µ—Ç –∑–∞—Å–µ–ª—ë–Ω (–ø–æ—Å–ª–µ –ø–µ—Ä–≤–æ–π –Ω–µ–¥–µ–ª–∏ —Å–µ–Ω—Ç—è–±—Ä—è)."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_docs', 'main'))
    elif data == 'dorm_cost':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–°–∫–æ–ª—å–∫–æ —Å—Ç–æ–∏—Ç –ø—Ä–æ–∂–∏–≤–∞–Ω–∏–µ –≤ –æ–±—â–µ–∂–∏—Ç–∏–∏?", callback_data='dorm_cost_price'),
            InlineKeyboardButton("–ù—É–∂–Ω–æ –ª–∏ –≤–µ–∑—Ç–∏ —Å —Å–æ–±–æ–π –ø–æ—Å—É–¥—É –∏ –ø–æ—Å—Ç–µ–ª—å–Ω–æ–µ –±–µ–ª—å–µ?", callback_data='dorm_cost_items'),
            InlineKeyboardButton("–ï—Å—Ç—å –ª–∏ –≤ –æ–±—â–µ–∂–∏—Ç–∏–∏ —Å—Ç–æ–ª–æ–≤–∞—è –∏–ª–∏ –Ω—É–∂–Ω–æ –≥–æ—Ç–æ–≤–∏—Ç—å —Å–∞–º–∏–º?", callback_data='dorm_cost_dining')
        )
        markup.add(*back_home_buttons('dorm_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'dorm_cost_price':
        text = "–°—Ç–æ–∏–º–æ—Å—Ç—å –ø—Ä–æ–∂–∏–≤–∞–Ω–∏—è –≤ –æ–±—â–µ–∂–∏—Ç–∏–∏ –∑–∞–≤–∏—Å–∏—Ç –æ—Ç –µ–≥–æ —Ç–∏–ø–∞ (–∫–æ—Ä–∏–¥–æ—Ä–Ω—ã–π –∏–ª–∏ –∫–≤–∞—Ä—Ç–∏—Ä–Ω—ã–π). –ê–∫—Ç—É–∞–ª—å–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –ø–æ –æ–ø–ª–∞—Ç–µ –æ–±—â–µ–∂–∏—Ç–∏–π –¥–æ—Å—Ç—É–ø–Ω–∞ –≤ —Ä–∞–∑–¥–µ–ª–µ –û–±—â–µ–∂–∏—Ç–∏—è –Ω–∞ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω–æ–º —Å–∞–π—Ç–µ –°–§–£."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–ê–∫—Ç—É–∞–ª—å–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –ø–æ –æ–±—â–µ–∂–∏—Ç–∏—è–º", url="https://sfu.ru/ru/about/campus/hostels"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('dorm_cost', 'main'))
    elif data == 'dorm_cost_items':
        text = "–ü–æ –∑–∞–ø—Ä–æ—Å—É –ø–æ—Å—Ç–µ–ª—å–Ω–æ–µ –±–µ–ª—å–µ –º–æ–∂–µ—Ç –±—ã—Ç—å –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ, –∞ –≤–æ—Ç –ø–æ—Å—É–¥—É –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –ø—Ä–∏–≤–µ–∑—Ç–∏ —Å–≤–æ—é."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_cost', 'main'))
    elif data == 'dorm_cost_dining':
        text = "–í –∑–¥–∞–Ω–∏—è—Ö –æ–±—â–µ–∂–∏—Ç–∏–π —Å—Ç–æ–ª–æ–≤—ã—Ö –Ω–µ—Ç, –Ω–æ –µ—Å—Ç—å –∫—É—Ö–Ω—è (–≤ –æ–±—â–µ–∂–∏—Ç–∏–∏ –∫–æ—Ä–∏–¥–æ—Ä–Ω–æ–≥–æ —Ç–∏–ø–∞ –∫—É—Ö–Ω—è –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –Ω–∞ —ç—Ç–∞–∂–µ).  –°—Ç–æ–ª–æ–≤—ã–µ –µ—Å—Ç—å  –≤ —É—á–µ–±–Ω—ã—Ö –∫–æ—Ä–ø—É—Å–∞—Ö, –∫–æ—Ç–æ—Ä—ã–µ –Ω–∞—Ö–æ–¥—è—Ç—Å—è –≤ –ø–µ—à–µ–π –¥–æ—Å—Ç—É–ø–Ω–æ—Å—Ç–∏ –æ—Ç –æ–±—â–µ–∂–∏—Ç–∏–π. –¢–∞–∫–∂–µ –Ω–∞ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏ –∫–∞–º–ø—É—Å–∞ –µ—Å—Ç—å –º–∞–≥–∞–∑–∏–Ω—ã."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_cost', 'main'))
    elif data == 'dorm_main':
        handle_dormitory(chat_id)

# Section: –£—á–µ–±–Ω—ã–π –ø—Ä–æ—Ü–µ—Å—Å
def handle_educational_process(chat_id):
    text = "–í —ç—Ç–æ–º —Ä–∞–∑–¥–µ–ª–µ –í—ã –Ω–∞–π–¥–µ—Ç–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —Ç–æ–º, –∫–∞–∫ –ø—Ä–æ—Ö–æ–¥—è—Ç –∑–∞–Ω—è—Ç–∏—è. –í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å:"
    markup = InlineKeyboardMarkup(row_width=1)
    buttons = [
        InlineKeyboardButton("–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –∑–∞–Ω—è—Ç–∏–π", callback_data='edu_schedule'),
        InlineKeyboardButton("–ú–µ—Å—Ç–æ —É—á–µ–±—ã –∏ –∏–Ω—Ñ—Ä–∞—Å—Ç—Ä—É–∫—Ç—É—Ä–∞", callback_data='edu_location')
    ]
    markup.add(*buttons)
    markup.add(InlineKeyboardButton("üîô –ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data='main'))
    bot.send_message(chat_id, text, reply_markup=markup)

def handle_edu_sub(chat_id, data):
    if data == 'edu_schedule':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–í–æ —Å–∫–æ–ª—å–∫–æ –Ω–∞—á–∏–Ω–∞—é—Ç—Å—è –∑–∞–Ω—è—Ç–∏—è?", callback_data='edu_sched_start'),
            InlineKeyboardButton("–£—á–∞—Ç—Å—è –ª–∏ —Å—Ç—É–¥–µ–Ω—Ç—ã –ø–æ —Å—É–±–±–æ—Ç–∞–º?", callback_data='edu_sched_sat'),
            InlineKeyboardButton("–í –∫–∞–∫—É—é —Å–º–µ–Ω—É –ø—Ä–æ—Ö–æ–¥—è—Ç –∑–∞–Ω—è—Ç–∏—è?", callback_data='edu_sched_shift'),
            InlineKeyboardButton("–ì–¥–µ –º–æ–∂–Ω–æ –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ?", callback_data='edu_sched_where'),
            InlineKeyboardButton("–°–∫–æ–ª—å–∫–æ –¥–ª–∏—Ç—Å—è –±–æ–ª—å—à–∞—è –ø–µ—Ä–µ–º–µ–Ω–∞?", callback_data='edu_sched_break')
        )
        markup.add(*back_home_buttons('edu_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'edu_sched_start':
        text = "–ü–µ—Ä–≤–∞—è –ø–∞—Ä–∞ –Ω–∞—á–∏–Ω–∞–µ—Ç—Å—è –≤ 8:30"
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_schedule', 'main'))
    elif data == 'edu_sched_sat':
        text = "–î–∞"
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_schedule', 'main'))
    elif data == 'edu_sched_shift':
        text = "–ó–∞–Ω—è—Ç–∏—è –ø—Ä–æ—Ö–æ–¥—è—Ç —Å–æ–≥–ª–∞—Å–Ω–æ —É—á–µ–±–Ω–æ–º—É —Ä–∞—Å–ø–∏—Å–∞–Ω–∏—é."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_schedule', 'main'))
    elif data == 'edu_sched_where':
        text = "–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ —É—á–µ–±–Ω—ã—Ö –∑–∞–Ω—è—Ç–∏–π —Ä–∞–∑–º–µ—â–∞–µ—Ç—Å—è –Ω–∞ —Å–∞–π—Ç–µ –ò–Ω—Å—Ç–∏—Ç—É—Ç–∞ –≤ —Ä–∞–∑–¥–µ–ª–µ ¬´–°—Ç—É–¥–µ–Ω—Ç—É¬ª, –Ω–∞ —Å–∞–π—Ç–µ –£–Ω–∏–≤–µ—Ä—Å–∏—Ç–µ—Ç–∞ –≤ —Ä–∞–∑–¥–µ–ª–µ ¬´–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ¬ª."
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(InlineKeyboardButton("–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –Ω–∞ —Å–∞–π—Ç–µ –ò–§–∏–Ø–ö", url="https://ifiyak.sfu-kras.ru/raspisanie/"))
        markup.add(InlineKeyboardButton("–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –Ω–∞ —Å–∞–π—Ç–µ –°–§–£", url="https://sfu.ru/ru/education/schedule"))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('edu_schedule', 'main'))
    elif data == 'edu_sched_break':
        text = "–ë–æ–ª—å—à–∞—è –ø–µ—Ä–µ–º–µ–Ω–∞ –¥–ª–∏—Ç—Å—è 35 –º–∏–Ω—É—Ç"
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_schedule', 'main'))
    elif data == 'edu_location':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –≤–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ü–æ –∫–∞–∫–æ–º—É –∞–¥—Ä–µ—Å—É –±—É–¥—É—Ç –ø—Ä–æ—Ö–æ–¥–∏—Ç—å –∑–∞–Ω—è—Ç–∏—è?", callback_data='edu_loc_address'),
            InlineKeyboardButton("–ï—Å—Ç—å –ª–∏ –≤ –∫–æ—Ä–ø—É—Å–∞—Ö —Å—Ç–æ–ª–æ–≤—ã–µ?", callback_data='edu_loc_dining')
        )
        markup.add(*back_home_buttons('edu_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'edu_loc_address':
        text = "–ó–∞–Ω—è—Ç–∏—è –ø—Ä–æ—Ö–æ–¥—è—Ç –ø—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–µ–Ω–Ω–æ –ø–æ –∞–¥—Ä–µ—Å—É –°–≤–æ–±–æ–¥–Ω—ã–π –ø—Ä–æ—Å–ø–µ–∫—Ç, 82–ê –ª–∏–±–æ –≤ —Å–æ—Å–µ–¥–Ω–∏—Ö –∫–æ—Ä–ø—É—Å–∞—Ö –Ω–∞ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏ –∫–∞–º–ø—É—Å–∞."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_location', 'main'))
    elif data == 'edu_loc_dining':
        text = "–î–∞"
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('edu_location', 'main'))
    elif data == 'edu_main':
        handle_educational_process(chat_id)

# Section: –í—ã–±–æ—Ä —è–∑—ã–∫–æ–≤—ã—Ö –≥—Ä—É–ø–ø
def handle_language_groups(chat_id):
    text = "–≠—Ç–æ—Ç —Ä–∞–∑–¥–µ–ª –ø—Ä–µ–¥–Ω–∞–∑–Ω–∞—á–µ–Ω –¥–ª—è –±—É–¥—É—â–∏—Ö –ª–∏–Ω–≥–≤–∏—Å—Ç–æ–≤ –∏ –ø–µ—Ä–µ–≤–æ–¥—á–∏–∫–æ–≤. –ó–¥–µ—Å—å –í—ã —É–∑–Ω–∞–µ—Ç–µ, –∫–∞–∫ –≤—ã–±—Ä–∞—Ç—å —è–∑—ã–∫ –¥–ª—è –∏–∑—É—á–µ–Ω–∏—è –∏ –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –≤ –Ω—É–∂–Ω—É—é –≥—Ä—É–ø–ø—É. –í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å:"
    markup = InlineKeyboardMarkup(row_width=1)
    buttons = [
        InlineKeyboardButton("–†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –ø–æ –≥—Ä—É–ø–ø–∞–º", callback_data='lang_distribution'),
        InlineKeyboardButton("–°–º–µ–Ω–∞ –≥—Ä—É–ø–ø—ã", callback_data='lang_change')
    ]
    markup.add(*buttons)
    markup.add(InlineKeyboardButton("üîô –ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data='main'))
    bot.send_message(chat_id, text, reply_markup=markup)

def handle_lang_sub(chat_id, data):
    if data == 'lang_distribution':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ö–∞–∫–∏–µ —è–∑—ã–∫–∏ –∏–∑—É—á–∞—é—Ç—Å—è –≤ –ò–§–∏–Ø–ö?", callback_data='lang_dist_languages'),
            InlineKeyboardButton("–ö–∞–∫ –ø–æ–ø–∞—Å—Ç—å –≤ –∫–∏—Ç–∞–π—Å–∫—É—é –≥—Ä—É–ø–ø—É?", callback_data='lang_dist_chinese'),
            InlineKeyboardButton("–£ –º–µ–Ω—è 61 –±–∞–ª–ª –ï–ì–≠ –ø–æ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º—É, –ø–æ–ø–∞–¥—É –ª–∏ —è –≤ –∫–∏—Ç–∞–π—Å–∫—É—é –≥—Ä—É–ø–ø—É?", callback_data='lang_dist_61'),
            InlineKeyboardButton("–ö–∞–∫ —É—á–∏—Ç—ã–≤–∞—é—Ç—Å—è –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—ã –ø—Ä–∏ –≤—ã–±–æ—Ä–µ —è–∑—ã–∫–∞?", callback_data='lang_dist_priorities')
        )
        markup.add(*back_home_buttons('lang_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'lang_dist_languages':
        text = "–ö–∏—Ç–∞–π—Å–∫–∏–π, —è–ø–æ–Ω—Å–∫–∏–π, –∏—Å–ø–∞–Ω—Å–∫–∏–π, –Ω–µ–º–µ—Ü–∫–∏–π, —Ñ—Ä–∞–Ω—Ü—É–∑—Å–∫–∏–π, —Ä—É—Å—Å–∫–∏–π –∫–∞–∫ –∏–Ω–æ—Å—Ç—Ä–∞–Ω–Ω—ã–π"
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('lang_distribution', 'main'))
    elif data == 'lang_dist_chinese':
        text = "–ù–∞–±—Ä–∞—Ç—å –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–π –±–∞–ª–ª –ï–ì–≠ –ø–æ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º—É —è–∑—ã–∫—É, —É–∫–∞–∑–∞–Ω–Ω—ã–π –≤ –ü–æ–ª–æ–∂–µ–Ω–∏–∏ –æ —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–∏ –ø–æ —è–∑—ã–∫–æ–≤—ã–º –≥—Ä—É–ø–ø–∞–º."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–û—Ç–∫—Ä—ã—Ç—å –ü–æ–ª–æ–∂–µ–Ω–∏–µ –≤ PDF", url=POLOZHENIE_PDF))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('lang_distribution', 'main'))
    elif data == 'lang_dist_61':
        text = "–£—Å–ª–æ–≤–∏–µ–º —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—è –≤ –∫–∏—Ç–∞–π—Å–∫—É—é –≥—Ä—É–ø–ø—É —è–≤–ª—è–µ—Ç—Å—è –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–π –±–∞–ª–ª –ï–ì–≠ –ø–æ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º—É —è–∑—ã–∫—É, —É–∫–∞–∑–∞–Ω–Ω—ã–π –≤ –ü–æ–ª–æ–∂–µ–Ω–∏–∏ –æ —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–∏ –ø–æ —è–∑—ã–∫–æ–≤—ã–º –≥—Ä—É–ø–ø–∞–º."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–û—Ç–∫—Ä—ã—Ç—å –ü–æ–ª–æ–∂–µ–Ω–∏–µ –≤ PDF", url=POLOZHENIE_PDF))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('lang_distribution', 'main'))
    elif data == 'lang_dist_priorities':
        text = "–ü–æ –ø–µ—Ä–≤–æ–º—É –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—É –∏ —Å–æ–≥–ª–∞—Å–Ω–æ –ü–æ–ª–æ–∂–µ–Ω–∏—é –æ —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—é –ø–æ —è–∑—ã–∫–æ–≤—ã–º –≥—Ä—É–ø–ø–∞–º."
        markup = InlineKeyboardMarkup()
        markup.add(InlineKeyboardButton("–û—Ç–∫—Ä—ã—Ç—å –ü–æ–ª–æ–∂–µ–Ω–∏–µ –≤ PDF", url=POLOZHENIE_PDF))
        bot.send_message(chat_id, text, reply_markup=markup)
        bot.send_message(chat_id, "üîô –ù–∞–∑–∞–¥", reply_markup=back_home_buttons('lang_distribution', 'main'))
    elif data == 'lang_change':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –≤–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ú–æ–∂–Ω–æ –ª–∏ –ø–æ–º–µ–Ω—è—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—ã–µ —è–∑—ã–∫–∏?", callback_data='lang_change_switch'),
            InlineKeyboardButton("–ú–æ–∂–Ω–æ –ª–∏ –ø–µ—Ä–µ–≤–µ—Å—Ç–∏—Å—å –∏–∑ –≥—Ä—É–ø–ø—ã –≤ –≥—Ä—É–ø–ø—É, –µ—Å–ª–∏ —è–∑—ã–∫ –Ω–µ –Ω—Ä–∞–≤–∏—Ç—Å—è?", callback_data='lang_change_dislike')
        )
        markup.add(*back_home_buttons('lang_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'lang_change_switch':
        text = "–î–∞, –Ω–æ –Ω–µ –ø–æ–∑–¥–Ω–µ–µ —á–µ–º —á–µ—Ä–µ–∑ 2 –Ω–µ–¥–µ–ª–∏ —Å –º–æ–º–µ–Ω—Ç–∞ –Ω–∞—á–∞–ª–∞ 1 —Å–µ–º–µ—Å—Ç—Ä–∞ –æ–±—É—á–µ–Ω–∏—è."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('lang_change', 'main'))
    elif data == 'lang_change_dislike':
        text = "–î–∞, –Ω–æ –Ω–µ –ø–æ–∑–¥–Ω–µ–µ —á–µ–º —á–µ—Ä–µ–∑ 2 –Ω–µ–¥–µ–ª–∏ —Å –º–æ–º–µ–Ω—Ç–∞ –Ω–∞—á–∞–ª–∞ 1 —Å–µ–º–µ—Å—Ç—Ä–∞ –æ–±—É—á–µ–Ω–∏—è."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('lang_change', 'main'))
    elif data == 'lang_main':
        handle_language_groups(chat_id)

# Section: –ö–æ–Ω—Ç–∞–∫—Ç—ã
def handle_contacts(chat_id):
    text = """–¢–µ–ª–µ—Ñ–æ–Ω: +79230153878
–í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã –ø—Ä–∏–µ–º–Ω–æ–π –∫–æ–º–∏—Å—Å–∏–∏: –ø–æ–Ω–µ–¥–µ–ª—å–Ω–∏–∫ ‚Äî –ø—è—Ç–Ω–∏—Ü–∞, —Å 9 –¥–æ 17."""
    markup = InlineKeyboardMarkup(row_width=2)
    markup.add(
        InlineKeyboardButton("üè† –í –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data='main'),
        InlineKeyboardButton("üìû –°–≤—è–∑–∞—Ç—å—Å—è —Å –∫–æ–º–∏—Å—Å–∏–µ–π", callback_data='contacts')
    )
    bot.send_message(chat_id, text, reply_markup=markup)

if __name__ == '__main__':
    bot.infinity_polling()
