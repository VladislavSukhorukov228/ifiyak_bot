# Section: –û–±—â–µ–∂–∏—Ç–∏–µ
def handle_dormitory(chat_id):
    text = "–ú—ã –ø–æ–Ω–∏–º–∞–µ–º, —á—Ç–æ –ø–µ—Ä–µ–µ–∑–¥ –≤ –¥—Ä—É–≥–æ–π –≥–æ—Ä–æ–¥ ‚Äî —ç—Ç–æ –≤–∞–∂–Ω—ã–π —à–∞–≥. –í —ç—Ç–æ–º —Ä–∞–∑–¥–µ–ª–µ –í—ã –Ω–∞–π–¥–µ—Ç–µ –≤—Å—é –Ω–µ–æ–±—Ö–æ–¥–∏–º—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ–± —É—Å–ª–æ–≤–∏—è—Ö –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–∏—è –æ–±—â–µ–∂–∏—Ç–∏–π –∏ –æ –∑–∞—Å–µ–ª–µ–Ω–∏–∏. –í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â—É—é –í–∞—Å —Ç–µ–º—É:"
    markup = InlineKeyboardMarkup(row_width=1)
    buttons = [
        InlineKeyboardButton("–ü—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –º–µ—Å—Ç", callback_data='dorm_provision'),
        InlineKeyboardButton("–î–æ–∫—É–º–µ–Ω—Ç—ã –∏ –∑–∞—Å–µ–ª–µ–Ω–∏–µ", callback_data='dorm_docs'),
        InlineKeyboardButton("–°—Ç–æ–∏–º–æ—Å—Ç—å –∏ –±—ã—Ç–æ–≤—ã–µ —É—Å–ª–æ–≤–∏—è", callback_data='dorm_cost')
    ]
    markup.add(*buttons)
    markup.add(InlineKeyboardButton("üîô –ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", callback_data='main'))
    bot.send_message(chat_id, text, reply_markup=markup)

def handle_dorm_sub(chat_id, data):
    if data == 'dorm_provision':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
        markup.add(
            InlineKeyboardButton("–ö–æ–º—É –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –æ–±—â–µ–∂–∏—Ç–∏–µ?", callback_data='dorm_prov_who'),
            InlineKeyboardButton("–ï—Å–ª–∏ —è –ø–æ—Å—Ç—É–ø–∏–ª –Ω–∞ –ø–ª–∞—Ç–Ω–æ–µ, –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –ª–∏ –º–Ω–µ –æ–±—â–µ–∂–∏—Ç–∏–µ?", callback_data='dorm_prov_paid'),
            InlineKeyboardButton("–ú–æ–∂–Ω–æ –ª–∏ —Å–∞–º–æ–º—É –≤—ã–±—Ä–∞—Ç—å –æ–±—â–µ–∂–∏—Ç–∏–µ?", callback_data='dorm_prov_choose'),
            InlineKeyboardButton("–ì–¥–µ –Ω–∞—Ö–æ–¥—è—Ç—Å—è –æ–±—â–µ–∂–∏—Ç–∏—è –ò–§–∏–Ø–ö?", callback_data='dorm_prov_location'),
            InlineKeyboardButton("–ú–æ–≥—É –ª–∏ —è –∑–∞—Å–µ–ª–∏—Ç—å—Å—è –≤ –æ–¥–Ω—É –∫–æ–º–Ω–∞—Ç—É —Å –¥—Ä—É–≥–æ–º –∏–ª–∏ –ø–æ–¥—Ä—É–≥–æ–π?", callback_data='dorm_prov_friend')
        )
        markup.add(*back_home_buttons('dorm_main', 'main').keyboard[0])
        bot.send_message(chat_id, text, reply_markup=markup)
    elif data == 'dorm_prov_who':
        text = "–û–±—â–µ–∂–∏—Ç–∏–µ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –≤—Å–µ–º –∏–Ω–æ–≥–æ—Ä–æ–¥–Ω–∏–º —Å—Ç—É–¥–µ–Ω—Ç–∞–º, –Ω–µ –∏–º–µ—é—â–∏–º –ø—Ä–æ–ø–∏—Å–∫–∏ –≤ –≥. –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–µ."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_provision', 'main'))
    elif data == 'dorm_prov_paid':
        text = "–û–±—â–µ–∂–∏—Ç–∏–µ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –≤—Å–µ–º –∏–Ω–æ–≥–æ—Ä–æ–¥–Ω–∏–º —Å—Ç—É–¥–µ–Ω—Ç–∞–º, –Ω–µ –∏–º–µ—é—â–∏–º –ø—Ä–æ–ø–∏—Å–∫–∏ –≤ –≥. –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–µ."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_provision', 'main'))
    elif data == 'dorm_prov_choose':
        text = "–ù–µ—Ç, —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Å—Ç –ø—Ä–æ–∏—Å—Ö–æ–¥–∏—Ç —Å–æ–≥–ª–∞—Å–Ω–æ —Ä–µ–π—Ç–∏–Ω–≥—É (—Å—É–º–º–∞ –±–∞–ª–ª–æ–≤ –ï–ì–≠)."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_provision', 'main'))
    elif data == 'dorm_prov_location':
        text = """–°—Ç—É–¥–µ–Ω—Ç—ã –ò–Ω—Å—Ç–∏—Ç—É—Ç–∞ —Ñ–∏–ª–æ–ª–æ–≥–∏–∏ –∏ —è–∑—ã–∫–æ–≤–æ–π –∫–æ–º–º—É–Ω–∏–∫–∞—Ü–∏–∏ –ø—Ä–æ–∂–∏–≤–∞—é—Ç –≤ —Å–ª–µ–¥—É—é—â–∏—Ö –æ–±—â–µ–∂–∏—Ç–∏—è—Ö (–∏ –≤—Å–µ –æ–Ω–∏ –Ω–∞—Ö–æ–¥—è—Ç—Å—è –Ω–∞ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏ –∫–∞–º–ø—É—Å–∞): 
¬∑ –ø—Ä. –°–≤–æ–±–æ–¥–Ω—ã–π, 80, –æ–±—â–µ–∂–∏—Ç–∏–µ ‚Ññ 17, 
¬∑ –ø—Ä. –°–≤–æ–±–æ–¥–Ω—ã–π, 78, –æ–±—â–µ–∂–∏—Ç–∏–µ ‚Ññ 18, 
¬∑ –ø—Ä. –°–≤–æ–±–æ–¥–Ω—ã–π, 76, –æ–±—â–µ–∂–∏—Ç–∏–µ ‚Ññ 19, 
¬∑ –ø—Ä. –°–≤–æ–±–æ–¥–Ω—ã–π, 76 ¬´–ê¬ª, ¬´–ì¬ª, –æ–±—â–µ–∂–∏—Ç–∏–µ ‚Ññ 20, 
¬∑ –ø—Ä. –°–≤–æ–±–æ–¥–Ω—ã–π, 76 ¬´–ù¬ª, –æ–±—â–µ–∂–∏—Ç–∏–µ ‚Ññ 21"""
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_provision', 'main'))
    elif data == 'dorm_prov_friend':
        text = "–ï—Å–ª–∏ –ø–æ–¥—Ä—É–∂–∫–∞ –ø–æ—Å—Ç—É–ø–∞–µ—Ç —Ç–∞–∫–∂–µ –≤ –ò–§–∏–Ø–ö –∏ —É –≤–∞—Å –ø—Ä–∏–º–µ—Ä–Ω–æ –æ–¥–∏–Ω–∞–∫–æ–≤—ã–µ –±–∞–ª–ª—ã (—Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –º–µ—Å—Ç –≤ –æ–±—â–µ–∂–∏—Ç–∏–∏ –∑–∞–≤–∏—Å–∏—Ç –æ—Ç —Å—É–º–º—ã –±–∞–ª–ª–æ–≤ –ï–ì–≠), —Ç–æ —Ç–∞–∫–∞—è –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å –µ—Å—Ç—å."
        bot.send_message(chat_id, text, reply_markup=back_home_buttons('dorm_provision', 'main'))
    elif data == 'dorm_docs':
        text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π –í–∞—Å –≤–æ–ø—Ä–æ—Å"
        markup = InlineKeyboardMarkup(row_width=1)
